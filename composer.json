{
    "name": "silverstripe/security-extensions",
    "description": "A temporary polyfill repository for security improvements that will be ported into core",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/admin": ">=1.0.0 <1.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SecurityExtensions\\": "src/",
            "SilverStripe\\SecurityExtensions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
