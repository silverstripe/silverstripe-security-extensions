{
    "name": "silverstripe/security-extensions",
    "description": "A temporary polyfill repository for security improvements that will be ported into core",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/admin": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "silverstripe/framework": "^5",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SecurityExtensions\\": "src/",
            "SilverStripe\\SecurityExtensions\\Tests\\": "tests/php/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}